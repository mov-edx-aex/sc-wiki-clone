.tt-blocks-advanced-column&lt;br /&gt;
{&lt;br /&gt;
  padding-top: 20px;&lt;br /&gt;
  padding-bottom: 20px;&lt;br /&gt;
  padding-left: 20px;&lt;br /&gt;
  padding-right: 20px;&lt;br /&gt;
  margin-top: 20px;&lt;br /&gt;
  margin-bottom: 20px;&lt;br /&gt;
  min-height: auto;&lt;br /&gt;
  display: flex;&lt;br /&gt;
  position: relative;&lt;br /&gt;
  &lt;br /&gt;
  background: #0693e3;&lt;br /&gt;
  --columns-width: 100%;&lt;br /&gt;
  --horizontal-align: unset;&lt;br /&gt;
  justify-content: &amp;quot;&amp;quot;;&lt;br /&gt;
  transition: 0.3s;&lt;br /&gt;
}&lt;br /&gt;
&lt;br /&gt;
