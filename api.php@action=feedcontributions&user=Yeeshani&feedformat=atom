.hidden&lt;br /&gt;
{&lt;br /&gt;
  display: none;&lt;br /&gt;
}&lt;br /&gt;
&lt;br /&gt;
/*END page adoption stuff*/&lt;br /&gt;
&lt;br /&gt;
/*resizing images for small screens*/&lt;br /&gt;
img { height: auto; max-width: 96%; }&lt;br /&gt;
&lt;br /&gt;
/*attempting to hide main page title*/&lt;br /&gt;
body.page-Main_Page.action-view h1.firstHeading, body.page-Main_Page.action-submit h1.firstHeading { display: none; }&lt;br /&gt;
&lt;br /&gt;
&lt;br /&gt;
/*TripTickFooter*/&lt;br /&gt;
.tt-innerblocks-wrap&lt;br /&gt;
{&lt;br /&gt;
    display: flex;&lt;br /&gt;
    flex-basis: 100%;&lt;br /&gt;
    word-break: keep-all;&lt;br /&gt;
    max-width: 100%;&lt;br /&gt;
    font-size: 16px;&lt;br /&gt;
    line-height: 1.5;&lt;br /&gt;
}&lt;br /&gt;
&lt;br /&gt;
.tt-has-small-font-size&lt;br /&gt;
{&lt;br /&gt;
  font-size: 12px !important;&lt;br /&gt;
}&lt;br /&gt;
&lt;br /&gt;
.tt-has-text-align-center&lt;br /&gt;
{&lt;br /&gt;
  text-align: center;&lt;br /&gt;
}&lt;br /&gt;
&lt;br /&gt;
.tt-blocks-advanced-column&lt;br /&gt;
{&lt;br /&gt;
  padding-top: 20px;&lt;br /&gt;
  padding-bottom: 20px;&lt;br /&gt;
  padding-left: 20px;&lt;br /&gt;
  padding-right: 20px;&lt;br /&gt;
  margin-top: 20px;&lt;br /&gt;
  margin-bottom: 20px;&lt;br /&gt;
  min-height: auto;&lt;br /&gt;
  display: flex;&lt;br /&gt;
  position: relative;&lt;br /&gt;
  &lt;br /&gt;
  background: #0693e3;&lt;br /&gt;
  --columns-width: 100%;&lt;br /&gt;
  --horizontal-align: unset;&lt;br /&gt;
  justify-content: &amp;quot;&amp;quot;;&lt;br /&gt;
  transition: 0.3s;&lt;br /&gt;
}&lt;br /&gt;
