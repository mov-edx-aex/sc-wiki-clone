		<summary type="html">&lt;p&gt;Yeeshani: Created page with &amp;quot;-- -- This module implements {{TextCopy}} -- require(&amp;#039;Module:No globals&amp;#039;)  local p = {}  function p.sidebar(frame, args) 	if not args then 		args = getArgs(frame) 	end 	local root = mw.html.create()          root = root:tag(&amp;#039;div&amp;#039;)     root         :addClass(&amp;#039;textcopyouter&amp;#039;)         :tag(&amp;#039;div&amp;#039;)            :addClass(&amp;#039;copyleft&amp;#039;)         :tag(&amp;#039;div&amp;#039;)            :addClass(&amp;#039;textcopyright&amp;#039;) end  return p&amp;quot;&lt;/p&gt;
&lt;hr /&gt;
&lt;div&gt;--&lt;br /&gt;
-- This module implements {{TextCopy}}&lt;br /&gt;
--&lt;br /&gt;
require(&#039;Module:No globals&#039;)&lt;br /&gt;
